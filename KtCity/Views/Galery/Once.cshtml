@model List<GaleryImgs>
@{
    
    Layout = "~/Views/Shared/_Layout.cshtml";
}

<div class="mainContainer shadows">
    <div class="container">
        <div class="container-images">
            
                @foreach (GaleryImgs g in Model)
                {
                    <a href="/Galery/@g.ImgName"
                       data-fancybox="gallery">
                        <img src="/Galery/@g.ImgName" alt="">
                    </a>
                }
           
        </div>
    </div>
</div>

